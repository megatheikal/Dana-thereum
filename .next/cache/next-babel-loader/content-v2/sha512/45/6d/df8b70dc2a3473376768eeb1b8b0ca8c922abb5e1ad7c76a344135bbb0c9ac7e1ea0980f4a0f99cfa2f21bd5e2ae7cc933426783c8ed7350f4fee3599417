nts[0] });\n      } catch (err) {\n        this.setState({ errorMessage: err.message });\n      }\n      this.setState({\n        nameOrganizer: \"\",\n        phoneOrganizer: \"\",\n        addressOrganizer: \"\",\n        emailOrganizer: \"\",\n        linkOrganizer: \"\",\n        campaignName: \"\",\n        campaignSummary: \"\",\n        campaignMinimum: \"\",\n        loading: false\n      });\n    } else {\n      console.log(this.state.errorMessage);\n    }\n  };\n\n  render() {\n    return (\n      <Layout>\n        <div className={form}>\n          <div className={box}>\n            <Form\n              size=\"huge\"\n              onSubmit={this.onSubmit}\n              error={!!this.state.errorMessage}\n            >\n              <Tab\n                menu={{\n                  fluid: true,\n                  vertical: true,\n                  tabular: true,\n                  attached: true\n                }}\n                panes={this.renderForm()}\n              />\n            </Form>\n          </div>\n        </div>\n      </Layout>\n    );\n  }\n}\n\nexport default CampaignNew;\n"]},"metadata":{},"sourceType":"module"}