function createCampaign(uint minimumn, string orgName, string orgAgencyName, string orgPhone, string orgAddress, string orgEmail, string orgLink, string refName, string refPhone, string refAddress, string refEmail, string refLink, string campName, string campSummary) public {
     address newCampaign = address (new Campaign(minimum, msg.sender, orgName, orgAgencyName, orgPhone, orgAddress, orgEmail, orgLink, refName,  refPhone,  refAddress,  refEmail,  refLink,  campName,  campSummary));
     deployedCampaigns.push(newCampaign);
     lastDeployedCampaign = newCampaign;
 }


 constructor (uint256  minimum, address  creator,string memory orgName, string memory orgAgencyName, string memory orgPhone, string memory orgAddress, string memory orgEmail, string memory orgLink, string memory refName, string memory refPhone, string memory refAddress, string memory refEmail, string memory refLink, string memory campName, string memory campSummary) {
    organiser = creator;
    minimumDonation=minimum;
    infoOrganiser[msg.sender]=Infoorganiser(orgName, orgAgencyName, orgPhone, orgAddress, orgEmail, orgLink);
    infoReference[msg.sender] = Inforeference(refName,  refPhone,  refAddress,  refEmail,  refLink);
    detailCampaign[msg.sender] = Detail(campName,campSummary);
}


function createCampaign(uint256  minimumn, string memory orgName, string memory orgAgencyName, string memory orgPhone, string memory orgAddress, string memory orgEmail, string memory orgLink, string memory refName, string memory refPhone, string memory refAddress, string memory refEmail, string memory refLink, string memory campName, string memory campSummary) public {
    address newCampaign = address (new Campaign(minimumn, msg.sender, orgName, orgAgencyName, orgPhone, orgAddress, orgEmail, orgLink, refName,  refPhone,  refAddress,  refEmail,  refLink,  campName,  campSummary));
    deployedCampaigns.push(newCampaign);
    lastDeployedCampaign = newCampaign;
}
